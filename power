public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		int ans=power(2,3);//to compute two raised to power 3
		System.out.println(ans);
	}
	public static int power(int x,int n)
	{
	    if(n==0)
	    {
	        return 1;
	    }
	    
	    int sa=power(x,n-1);
	    return x*sa;
	}
}
