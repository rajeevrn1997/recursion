import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
		int ans=fib(n);
		System.out.println(ans);
	}
	public static int fib(int n)
	{
	    if(n==1 ||n==2)
	    {
	        return 1;
	    }
	    
	    return fib(n-1)+fib(n-2);
	}
}
